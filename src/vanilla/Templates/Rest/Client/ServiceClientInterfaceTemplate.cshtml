@using System
@using System.Linq
@using AutoRest.Core.Utilities
@using AutoRest.CSharp.Model
@inherits AutoRest.Core.Template<CodeModelCs>
@Header("// ")
@EmptyLine
namespace @(Settings.Namespace).Api
{
@foreach (var usingString in Model.Usings) {
    @:using @usingString;
}
	using System.Threading;
	using System.Threading.Tasks;
	using RetailSuccess.SDK.Core;
	using RetailSuccess.SDK.Core.Models;
@EmptyLine
    /// <summary>
    @WrapComment("/// ", Model.Documentation.EscapeXmlComment())
    /// </summary>
    public partial interface I@(Model.ClientName)
    {
    
    @foreach(MethodCs method in Model.FilteredMethods.Where( each => each.Group.IsNullOrEmpty()) )
    {
        if (method.ExcludeFromInterface)
        {
            continue;
        }

        @:Task<@(method.OperationResponseReturnTypeString)> @(method.CorrectedName)Async(@(method.GetAsyncMethodParameterDeclaration(false, true)));
        @EmptyLine
        @:
    }

    }
}