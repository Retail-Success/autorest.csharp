@using System.Linq
@using AutoRest.Core.Utilities
@using AutoRest.CSharp.Model
@using AutoRest.CSharp.vanilla.Templates.Rest.Client
@inherits AutoRest.Core.Template<CodeModelCs>
@Header("// ")
@EmptyLine
namespace @(Settings.Namespace).Api
{
	using System;
	using System.Collections.Generic;
	using System.Net.Http;
	using System.Threading.Tasks;
	using RetailSuccess.Extensions.ApiClients.Abstractions;
	using RetailSuccess.SDK.Core;
	using RetailSuccess.SDK.Core.Models;
	using @(Settings.Namespace).Models;
@EmptyLine
	public class @Model.ClientName : ClientBase
	{
	@(Include(new ServiceClientBodyTemplate(), Model))

	@foreach (MethodCs method in Model.FilteredMethods.Where(m => m.Group.IsNullOrEmpty()))
	{
		@:@(Include(new MethodTemplate(), method))
		@EmptyLine
		@:
	}
	}
}